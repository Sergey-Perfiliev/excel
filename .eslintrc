{
    "parser": "babel-eslint",
    "env" : {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parents": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        //
				"linebreak-style": "off",
				"no-tabs": "off",
				"no-trailing-spaces": "off",
				"padded-blocks": "off",
				"indent": "off",
				"arrow-parens": "off"
				
    },
    "extends": ["eslint:recommended", "google"]
}